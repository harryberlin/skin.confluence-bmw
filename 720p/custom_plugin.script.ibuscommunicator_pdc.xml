          <posx>180</posx>
          <posy>0</posy>
          <height>30</height>
          <width>80</width>
          <label>$INFO[Window(10000).Property(IBUS_PDC_RRC)]</label>
          <align>center</align>
          <aligny>center</aligny>
          <font>font35_title</font>
          <textcolor_off>white</textcolor_off>
          <shadowcolor_off>grey2</shadowcolor_off>
          <textcolor>$VAR[TextColor1]</textcolor>
          <shadowcolor>black</shadowcolor>
        </control>
        <control type="label">
          <description>RRS</description>
          <posx>250</posx>
          <posy>0</posy>
          <height>30</height>
          <width>80</width>
          <label>$INFO[Window(10000).Property(IBUS_PDC_RRS)]</label>
          <align>center</align>
          <aligny>center</aligny>
          <font>font35_title</font>
          <textcolor_off>white</textcolor_off>
          <shadowcolor_off>grey2</shadowcolor_off>
          <textcolor>$VAR[TextColor1]</textcolor>
          <shadowcolor>black</shadowcolor>
        </control>
      </control>
      <!--Sensor Value Images-->
      <control type="group">
        <description>Front Sensor Images</description>
        <posx>0</posx>
        <posy>0</posy>
        <control type="multiimage">
          <description>FLS</description>
          <posx>45</posx>
          <posy>60</posy>
          <width>63</width>
          <height>120</height>
          <fadetime>0</fadetime>
          <timeperimage>2000</timeperimage>
          <randomize>true</randomize>
          <imagepath>bmw\pdc\side\SIDE$INFO[Window(10000).Property(IBUS_PDC_FLS_IMG)].png</imagepath>
        </control>
        <control type="multiimage">
          <description>FLC</description>
          <posx>119</posx>
          <posy>58</posy>
          <width>65</width>
          <height>118</height>
          <fadetime>0</fadetime>
          <timeperimage>2000</timeperimage>
          <randomize>true</randomize>
          <imagepath>bmw\pdc\mid\MID$INFO[Window(10000).Property(IBUS_PDC_FLC_IMG)].png</imagepath>
        </control>
        <control type="multiimage">
          <description>FRC</description>
          <posx>193</posx>
          <posy>58</posy>
          <width>65</width>
          <height>118</height>
          <fadetime>0</fadetime>
          <timeperimage>2000</timeperimage>
          <randomize>true</randomize>
          <imagepath flipx="true">bmw\pdc\mid\MID$INFO[Window(10000).Property(IBUS_PDC_FRC_IMG)].png</imagepath>
        </control>
        <control type="multiimage">
          <description>FRS</description>
          <posx>268</posx>
          <posy>60</posy>
          <width>63</width>
          <height>120</height>
          <fadetime>0</fadetime>
          <timeperimage>2000</timeperimage>
          <randomize>true</randomize>
          <imagepath flipx="true">bmw\pdc\side\SIDE$INFO[Window(10000).Property(IBUS_PDC_FRS_IMG)].png</imagepath>
        </control>
      </control>
      <control type="group">
        <description>Back Sensor Images</description>
        <posx>0</posx>
        <posy>450</posy>
        <control type="multiimage">
          <description>RLS</description>
          <posx>45</posx>
          <posy>60</posy>
          <width>63</width>
          <height>120</height>
          <fadetime>0</fadetime>
          <timeperimage>2000</timeperimage>
          <randomize>true</randomize>
          <imagepath flipy="true">bmw\pdc\side\SIDE$INFO[Window(10000).Property(IBUS_PDC_RLS_IMG)].png</imagepath>
        </control>
        <control type="multiimage">
          <description>RLC</description>
          <posx>119</posx>
          <posy>64</posy>
          <width>65</width>
          <height>118</height>
          <fadetime>0</fadetime>
          <timeperimage>2000</timeperimage>
          <randomize>true</randomize>
          <imagepath flipy="true">bmw\pdc\mid\MID$INFO[Window(10000).Property(IBUS_PDC_RLC_IMG)].png</imagepath>
        </control>
        <control type="multiimage">
          <description>RRC</description>
          <posx>193</posx>
          <posy>64</posy>
          <width>65</width>
          <height>118</height>
          <fadetime>0</fadetime>
          <timeperimage>2000</timeperimage>
          <randomize>true</randomize>
          <imagepath flipx="true" flipy="true">bmw\pdc\mid\MID$INFO[Window(10000).Property(IBUS_PDC_RRC_IMG)].png</imagepath>
        </control>
        <control type="multiimage">
          <description>RRS</description>
          <posx>268</posx>
          <posy>60</posy>
          <width>63</width>
          <height>120</height>
          <fadetime>0</fadetime>
          <timeperimage>2000</timeperimage>
          <randomize>true</randomize>
          <imagepath flipx="true" flipy="true">bmw\pdc\side\SIDE$INFO[Window(10000).Property(IBUS_PDC_RRS_IMG)].png</imagepath>
        </control>
      </control>
    </control>
  </controls>
  <!--Set Test-Values on Load-->
  <visible_off>True</visible_off>
  <onload_off>SetProperty(IBUS_PDC_FLS_IMG,bmw\pdc\side\,10000)</onload_off>
  <onload_off>SetProperty(IBUS_PDC_FLC_IMG,bmw\pdc\mid\,10000)</onload_off>
  <onload_off>SetProperty(IBUS_PDC_FRC_IMG,bmw\pdc\mid\,10000)</onload_off>
  <onload_off>SetProperty(IBUS_PDC_FRS_IMG,bmw\pdc\side\,10000)</onload_off>
  <onload_off>SetProperty(IBUS_PDC_RLS_IMG,bmw\pdc\side\,10000)</onload_off>
  <onload_off>SetProperty(IBUS_PDC_RLC_IMG,bmw\pdc\mid\,10000)</onload_off>
  <onload_off>SetProperty(IBUS_PDC_RRC_IMG,bmw\pdc\mid\,10000)</onload_off>
  <onload_off>SetProperty(IBUS_PDC_RRS_IMG,bmw\pdc\side\,10000)</onload_off>
